<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry
		location="F:\Workspaces\mysql-connector-java-5.1.18-bin.jar" />
	<context id="context1">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库链接URL、用户名、密码 -->
		<jdbcConnection connectionURL="jdbc:mysql://localhost:3306/attendance"
			driverClass="com.mysql.jdbc.Driver" password="123456" userId="root" />
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="fzu.edu.model"
			targetProject="AttendanceWeb/src/main/java" />
		<!-- 生成映射文件的包名和位置 -->
		<sqlMapGenerator targetPackage="fzu.edu.mapping"
			targetProject="AttendanceWeb/src/main/java" />
		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator targetPackage="fzu.edu.dao"
			targetProject="AttendanceWeb/src/main/java" type="XMLMAPPER" />

		<table tableName="sys_user" domainObjectName="User"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false" />
	</context>
</generatorConfiguration>